  <ReconQueries ClassName="ReconQuery" ClassDescription="Queries to run as part of recon reports">
    <!-- Execution Types -->

    <ExecutionTypes>
	  <!-- Queries for demo purposes -->
		<ExecutionType TypeName="EIM_DEV_FUND_CLASSES" TypeDescription="This execution returns the results of an ad-hoc query as a datareader" TransactionRequired="False">
			<ExecutionActions>
				<ExecutionAction ID="EIM_DEV_FUND_CLASSES" ConnectionStringKey="EIM_Connection_String" Text="select 
                                f.fund_id,
                                c.class_id,
                                c.class,
                                c.class_description,
                                v.value_long_name fund_type,
                                TRIM(both ' ' FROM f.fiscal_year_end) fiscal_year_end
                            from eim_fund_master f
                            join eim_fund_class c
                            on f.fund_id = c.fund_id
                            JOIN eim_code_value v
                            ON v.code_short_name = 'FS TEMPLATE'
                            and f.fund_type = v.value_short_name
                            WHERE f.fund_id not like 'D%'
                            order by f.fund_name, c.class" Type="SQLStatement" TransactionSequenceNumber="0"/>
			</ExecutionActions>
		</ExecutionType>
		<ExecutionType TypeName="FS_FUND_CLASSES" TypeDescription="This execution returns the results of an ad-hoc query as a datareader" TransactionRequired="False">
			<ExecutionActions>
				<ExecutionAction ID="FS_FUND_CLASSES" ConnectionStringKey="FS_Connection_String" Text="select
									f.fund_id,
									f.fund_number, --class_id
									f.class_name, --class
									f.short_name, --class_description
									t.code_name, --fund_type
									ltrim(rtrim(f.end_of_fiscal_year)) fiscal_year_end 
								from fund f
								join type_code t
								on f.fund_type = t.code_id
								where f.fund_number is not null and f.fund_number &lt;&gt; 'None'" Type="SQLStatement" TransactionSequenceNumber="0"/>
			</ExecutionActions>
		</ExecutionType>
		<ExecutionType TypeName="EIM_QA_FUND_CLASSES_SP" TypeDescription="Returns fund classes matching parameters provided." TransactionTimeoutMilliseconds="0" TransactionRequired="False">
			<ExecutionActions>
				<ExecutionAction ID="EIM_QA_FUND_CLASSES_SP" ConnectionStringKey="EIM_QA_Connection_String" Text="PKG_EIM_FUND_CLASS.PRC_GET_FUND_CLASS" Type="StoredProcedure" TransactionSequenceNumber="0"/>
			</ExecutionActions>
		</ExecutionType>
		<ExecutionType TypeName="EIM_DEV_FUND_SYMBOLS" TypeDescription="This execution returns the results of an ad-hoc query as a datareader" TransactionRequired="False">
			<ExecutionActions>
				<ExecutionAction ID="EIM_DEV_FUND_SYMBOLS" ConnectionStringKey="EIM_Connection_String" Text="select 
									class_id,
									class_description, 
									cusip, 
									symbol
								from eim_fund_class c
								where c.fund_id not like 'D%'" Type="SQLStatement" TransactionSequenceNumber="0"/>
			</ExecutionActions>
		</ExecutionType>
		<ExecutionType TypeName="FS_FUND_SYMBOLS" TypeDescription="This execution returns the results of an ad-hoc query as a datareader" TransactionRequired="False">
			<ExecutionActions>
				<ExecutionAction ID="FS_FUND_SYMBOLS" ConnectionStringKey="FS_Connection_String" Text="select
									f.fund_number, --class_id
									fe.fund_id fs_fund_id, 
									fe.cusip,
									fe.nasdaq_symbol --symbol
								from fund_external fe
								join fund f
								on fe.fund_id = f.fund_id
								where f.fund_number is not null and f.fund_number &lt;&gt; 'None'" Type="SQLStatement" TransactionSequenceNumber="0"/>
			</ExecutionActions>
		</ExecutionType>

