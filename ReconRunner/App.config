      <ExecutionType TypeName="EIM_FI_PORT_ALLOC" TypeDescription="This execution returns the results of an ad-hoc query as a datareader" TransactionRequired="False">
        <ExecutionActions>
          <ExecutionAction ID="Get_EIM_FI_PORT_ALLOC" ConnectionStringKey="EIM_Connection_String" Text="select   trim(fx.fund_xref_id) as fund_xref_id,
                                    trim(fd.fund_id) as fund_id,
                                    trim(fm.fund_name) as fund_name,
                                    fd.effective_date,
                                    cv.value_short_name,
                                    trim(fd.dimension_node) as dimension_node,
                                    fd.dimension_percentage
                           from     eim_fund_dimension fd 
                                        inner join eim_fund_xref fx on fx.fund_id = fd.fund_id
                                        inner join eim_code_value cv on cv.value_long_name = fd.dimension_node
                                        inner join eim_fund_master fm on fm.fund_id = fd.fund_id
                           where    fd.create_source = 'FI'
                                    and fx.fund_xref_type = 'FI'
                                    and fd.effective_date = (to_date('05/31/2007', 'mm/dd/yyyy'))   --Variable
                                    and cv.source_short_name = 'FI'
                                    and dimension_name = 'PORTFOLIO CODE'
                           order by fx.fund_xref_id, fd.dimension_node" Type="SQLStatement" TransactionSequenceNumber="0"/>
        </ExecutionActions>
      </ExecutionType>
